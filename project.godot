window/name="World War Swarm"

[dotnet]

project/assembly_name="Neuro Warriors"

[editor_plugins]

enabled=PackedStringArray("res://addons/neuro-sdk/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
